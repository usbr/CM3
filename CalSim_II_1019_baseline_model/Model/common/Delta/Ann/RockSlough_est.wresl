    condition abs(RS_m_3_sv) <= 0.001 .and. int(RS_b_3_sv) == 0
    value 0.}
  case NegSlope{
    condition RS_m_3_sv <= 0.0
    value 0.}
  case highcost{
    condition RS_Sac_ZeroExp_sv2 >= ANNcap
    value 0.}
  case LargeNegInt{
    condition RS_b_3_sv < -10000.0
