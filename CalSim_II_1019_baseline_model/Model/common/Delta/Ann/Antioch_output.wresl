!****************************************************************************************

! maximum export given the sacramento river flow

define AN_MaxExport {lower unbounded upper unbounded kind 'FLOW' units 'CFS'}
goal setANMax {
  lhs AN_MaxExport
  case NoSalinityControlPossible{
    condition abs(AN_m_sv) <= 0.001 .and. int(AN_b_sv) == 0
    rhs ExportCap}
  case NegSlope{
    condition AN_m_sv <= 0.0
    rhs 999999.}
  case HighCost{
    condition AN_Sac_ZeroExp_sv2 >= ANNcap
    rhs ExportCap}
  /*case NegCarriage{
    condition abs(AN_m_sv) >= 1.0
    rhs 999999.}*/
  case Normal {
    condition always
    rhs AN_m_sv*c400_ANN + AN_m_sv*c157 + AN_b_sv}
}



!****************************************************************************************

! required delta outflow given exports

define AN_MRDO {lower unbounded upper unbounded kind 'FLOW' units 'CFS'}
goal setANmrdo {
  lhs AN_MRDO
  case NoSalinityControlPossible{
    condition abs(AN_m_sv) <= 0.001 .and. int(AN_b_sv) == 0
    rhs 0.}
  case NegSlope{
    condition AN_m_sv <= 0.0
    rhs 0.}
  case HighCost{
    condition AN_Sac_ZeroExp_sv2 >= ANNcap
    rhs 0.}
  case Normal {
    condition always
    rhs ((1-AN_m_sv)/AN_m_sv)*ExportActualTD - AN_b_sv/AN_m_sv + C644 + C508 + C504 + I406 - D408 - D416 + D446A - C402B - net_delta_cuDV}
}

!****************************************************************************************

! required sac flow given exports

define AN_ReqSac {lower unbounded upper unbounded kind 'FLOW' units 'cfs'}
goal set_AN_ReqSac {
  lhs AN_ReqSac
  case NoSalinityControlPossible{
    condition abs(AN_m_sv) <= 0.001 .and. int(AN_b_sv) == 0
    rhs 0.}
  case NegSlope{
    condition AN_m_sv <= 0.0
    rhs 0.}
  case HighCost{
    condition AN_Sac_ZeroExp_sv2 >= ANNcap
    rhs 0.}
  case Normal {
    condition always
    rhs ExportActualTD/AN_m_sv - AN_b_sv/AN_m_sv}
}

