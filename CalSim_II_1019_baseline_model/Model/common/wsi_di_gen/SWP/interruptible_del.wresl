! Interuptible Deliveries

define swpdel_pin27{

       case      JAN  {
       condition month == JAN .and. wateryear > 1922
       sum(i=-1,-12,-1) D27_pin(i) * cfs_taf(i) }

       case      otherwise {
       condition always
       value     0 }
       }

