! define values based on Table A entitlements
define tableA_ag   {select Ag_entitlement  from TableA where Factor=1}
define tableA_mwd  {select MWD_entitlement from TableA where Factor=1}
define tableA_oth  {select OTH_entitlement from TableA where Factor=1}
!define tableA_int  {select INT_entitlement from TableA where Factor=1}

! get the annual requests -- these have been assembled into a lookup table - essentially 'tryswp.dat'
define req_tot_swp {select Total from AnnualReqDel_swp where year=calendaryear}
define req_ag_swp  {select AG    from AnnualReqDel_swp where year=calendaryear}
define req_mwd_swp {select MWD   from AnnualReqDel_swp where year=calendaryear}
define req_oth_swp {select OTH   from AnnualReqDel_swp where year=calendaryear}

define perdel_ag   {value req_ag_swp/tableA_ag}
define perdel_mwd  {value req_mwd_swp/tableA_mwd}
define perdel_oth  {value req_oth_swp/tableA_oth}

