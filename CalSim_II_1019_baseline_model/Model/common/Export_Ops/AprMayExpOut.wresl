/*
goal setD418_TD_P_4 {
     lhs D418_TD_P
    case APRMAY {
     condition month == APR .or. month == MAY
     rhs 800.
     lhs>rhs    penalty 0
     lhs<rhs    penalty 9999
}
}
*/

goal setD418_TD_NP_1 {
     lhs D418_TD_NP
    case APR {
     condition month == APR
     rhs D418*30./14. - D418_TD_P*16./14.
}
    case MAY {
     condition month==MAY
     rhs D418*31./16. - D418_TD_P*15./16.
}
    case other {
     condition always
     rhs 0.
}
}

