/*
Main WRESL file for CONV step WSI retraining
*/

SEQUENCE CYCLE1 {
    model   SJRBASE
    order   1
    }
SEQUENCE CYCLE2 {
    model   SJR_WQ1
    order   2
    }
SEQUENCE CYCLE3 {
    model   SJR_PULSE
     condition month==APR .or. month==MAY
    order   3
    }
SEQUENCE CYCLE4 {
    model   SJR_WQ2
     condition month==APR .or. month==MAY
    order   4
    }
SEQUENCE CYCLE5 {
    model   VAMP_AND_DO
     condition month>=APR .and. month<=SEP
    order   5
    }
SEQUENCE CYCLE6 {
    model   PRESETUP
    order   6
    }    
SEQUENCE CYCLE7 {
    model   SETUP
    order   7
    }
SEQUENCE CYCLE8 {
    model   MONTHLYWEIGHTED
    order   8
    }
SEQUENCE CYCLE9 {
    model   WHEELCVC
    order   9
    }

